{
  "canisters": {
    "escrow": {
      "candid": "src/escrow/escrow.did",
      "wasm": "target/wasm32-unknown-unknown/release/escrow.wasm",
      "package": "escrow",
      "type": "custom",
      "metadata": [
        {
          "name": "candid:service"
        }
      ],
      "build": [
        "bash src/escrow/build.sh",
        "candid-extractor target/wasm32-unknown-unknown/release/escrow.wasm > src/escrow/escrow.did"
      ],
      "init_arg": "(variant { regtest }, opt record { solana_network = opt variant { Custom = record { url = \"https://api.devnet.solana.com\"; headers = null } }; ed25519_key_name = opt variant { LocalDevelopment }; sol_rpc_canister_id = opt principal \"tghme-zyaaa-aaaar-qarca-cai\"; solana_commitment_level = null })"
    },
    "sol_rpc": {
      "specified_id": "tghme-zyaaa-aaaar-qarca-cai",
      "candid": "https://github.com/dfinity/sol-rpc-canister/releases/download/v1.0.0/sol_rpc_canister.did",
      "package": "sol_rpc_canister",
      "type": "custom",
      "wasm": "https://github.com/dfinity/sol-rpc-canister/releases/download/v1.0.0/sol_rpc_canister.wasm.gz",
      "metadata": [
        {
          "name": "candid:service"
        }
      ],
      "init_arg": "( record { manageApiKeys = null; overrideProvider = null; logFilter = opt variant { ShowAll }; numSubnetNodes = null; mode = opt variant { Demo }; } )",
      "remote": {
        "id": {
          "ic": "tghme-zyaaa-aaaar-qarca-cai",
          "playground": "tghme-zyaaa-aaaar-qarca-cai"
        }
      }
    },
    "evm_rpc": {
      "candid": "https://github.com/internet-computer-protocol/evm-rpc-canister/releases/latest/download/evm_rpc.did",
      "init_arg": "(record { })",
      "remote": {
        "id": {
          "ic": "7hfb6-caaaa-aaaar-qadga-cai"
        }
      },
      "specified_id": "7hfb6-caaaa-aaaar-qadga-cai",
      "type": "custom",
      "wasm": "https://github.com/internet-computer-protocol/evm-rpc-canister/releases/latest/download/evm_rpc.wasm.gz"
    },
    "frontend": {
      "dependencies": ["escrow"],
      "source": ["src/frontend/dist"],
      "type": "assets",
      "workspace": "nara_frontend"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    },
    "bitcoin": {
      "enabled": true,
      "nodes": ["127.0.0.1:18444"]
    }
  },
  "output_env_file": ".env",
  "networks": {
    "local": {
      "bind": "127.0.0.1:4943",
      "type": "ephemeral"
    },
    "ic": {
      "providers": ["https://ic0.app"],
      "type": "persistent"
    }
  },
  "version": 1
}
